{
  "homepage": "/",
  "name": "ivp-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "animate.css": "^3.7.2",
    "baffle-react": "0.0.2",
    "node-sass": "^4.13.1",
    "react": "^16.13.1",
    "react-animated-css": "^1.2.1",
    "react-bootstrap": "^1.0.0-beta.17",
    "react-countup": "^4.3.3",
    "react-dom": "^16.13.1",
    "react-flexbox-grid": "^2.1.2",
    "react-in-viewport": "^1.0.0-alpha.11",
    "react-lightgallery": "^0.4.3",
    "react-particles-js": "^2.7.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-scrolllock": "^5.0.1",
    "react-scrollspy": "^3.4.2",
    "react-slick": "^0.25.2",
    "scroll-to-element": "^2.0.3",
    "slick-carousel": "^1.8.1",
    "stylelint": "^13.3.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-scss": "^3.16.1",
    "typewriter-effect": "^2.13.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "babel-eslint": "10.1.0",
    "bootstrap": "^4.4.1",
    "font-awesome": "4.7.0",
    "gh-pages": "^2.2.0",
    "modern-normalize": "0.6.0"
  },
  "stylelint": {
    "plugins": [
      "stylelint-scss"
    ],
    "extends": "stylelint-config-recommended",
    "rules": {
      "no-descending-specificity": null,
      "at-rule-no-unknown": null,
      "scss/at-rule-no-unknown": null
    }
  },
  "scripts": {
    "css": "stylelint src/assets/scss/*/*.scss &&  node-sass --output-style compressed src/assets/scss/main.scss -o public/",
    "watch:css": "onchange \"/src/assets/scss/*.scss\" -- npm run css ",
    "develop": "npm run css && react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
